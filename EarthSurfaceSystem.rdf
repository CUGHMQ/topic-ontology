        <rdfs:hasChineseName>植物物候</rdfs:hasChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ontologies/202310/earthsurfacesystem#PlantPhysiologicalIndicators -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/202310/earthsurfacesystem#PlantPhysiologicalIndicators">
